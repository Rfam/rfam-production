env.PYTHONPATH="$baseDir"

profiles {

    local {
        process {
            executor = 'local'
        }
    }

    cluster {
        process {
            executor = 'lsf'
            maxForks = 2000
            scratch = true
        }
        executor {
            name = 'lsf'
            queueSize = 10000
        }
    }
}